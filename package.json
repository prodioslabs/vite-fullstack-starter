{
  "name": "vite-fullstack-starter",
  "version": "0.1",
  "description": "React fullstack starter with vite, nestjs and ts-rest",
  "private": true,
  "scripts": {
    "clean": "rimraf node_modules **/*/node_modules",
    "dev": "turbo dev",
    "build": "turbo build",
    "lint": "turbo lint",
    "format": "turbo format",
    "typecheck": "turbo typecheck",
    "typecheck:watch": "turbo typecheck:watch"
  },
  "keywords": [
    "vite",
    "nestjs",
    "react",
    "ts-rest",
    "starter"
  ],
  "author": "Team Prodios",
  "devDependencies": {
    "@repo/config": "workspace:*",
    "rimraf": "^6.0.1",
    "turbo": "2.5.0",
    "typescript": "5.4.5"
  },
  "packageManager": "pnpm@9.0.6",
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@babel/*",
        "expo-modules-*",
        "typescript"
      ]
    }
  }
}
